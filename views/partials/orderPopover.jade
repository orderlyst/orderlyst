ion-popover-view
    ion-content.text-center
        .item.metadata Your Order Code is <b>{{order.code}}</b>
        .item(ng-click="openWhatsApp()")
          i.icon.ion-social-whatsapp-outline
          | &nbsp;Share to WhatsApp
        .item(ng-click="openFacebookShareLink(order.code)")
          i.icon.ion-social-facebook
          | &nbsp;Share on Facebook
